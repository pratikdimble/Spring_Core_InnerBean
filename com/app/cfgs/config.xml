<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"  
	xmlns:p="http://www.springframework.org/schema/p"     
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    
 	xsi:schemaLocation="http://www.springframework.org/schema/beans      
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 
	
		
	<bean class="com.app.entity.Employee" name="emp">
	<property name="eid" value="111"/>
			<property name="ename" value="Pratik"/>
			<property name="data">
				<set>
					<value>11</value>
					<value>12</value>
				</set>
			</property>
		<property name="addr">
		<!-- Inner Bean -->
			<bean class="com.app.entity.Address" name="addr" >
				<property name="aid" value="1001"/>
					<property name="loc" value="Maharashtra"/>
					<property name="city">
				<list>
					<value>Pune</value>
					<value>Mumbai</value>
				</list>
				</property>
				</bean>
			</property>
		
			
	</bean>

	</beans>
